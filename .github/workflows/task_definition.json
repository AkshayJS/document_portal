{
  "family": "documentportaltd",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::017205512075:user/ecsTaskExecutionRole",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "1024",
  "memory": "8192",
  "containerDefinitions": [
    {
      "name": "document-portal-container",
      "image": "017205512075.dkr.ecr.eu-west-1.amazonaws.com/documentportal:fc236695dccf689130230d410df0397d61690bc0",
      "cpu": 1024,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp",
          "name": "document-portal-container-8080-tcp",
          "appProtocol": "http"
        }
      ],
      "environment": [
        {
          "name": "LANGCHAIN_PROJECT",
          "value": "DOCUMENT PORTAL"
        }
      ],
      "secrets": [
        {
          "name": "GROQ_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:eu-west-1:017205512075:secret:api_keys-bhRe1j:GROQ_API_KEY"
        },
        
        {
          "name": "GOOGLE_API_KEY",
          "valueFrom": "arn:aws:secretsmanager:eu-west-1:017205512075:secret:api_keys-bhRe1j:GOOGLE_API_KEY"
        }
        
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/documentportaltd",
          "awslogs-region": "eu-west-1",
          "awslogs-stream-prefix": "ecs",
          "mode": "non-blocking",
          "max-buffer-size": "25m"
        }
      }
    }
  ]
}
